# AWS AgentCore Demo Configuration
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
# Optional: Use AWS_PROFILE instead of access keys
# AWS_PROFILE=your-aws-profile

# AgentCore Configuration
AGENTCORE_ENDPOINT=https://agentcore.us-east-1.amazonaws.com

# Third-party Integration Credentials (for Identity demo)
# GitHub OAuth App credentials
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Salesforce Connected App credentials
SALESFORCE_CLIENT_ID=your_salesforce_client_id
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret

# Slack Bot Token
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# Observability Configuration
# OpenTelemetry endpoint (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=https://your-otel-collector-endpoint

# Datadog API Key (optional)
DATADOG_API_KEY=your_datadog_api_key

# LangSmith API Key (optional)
LANGSMITH_API_KEY=your_langsmith_api_key

# Demo Configuration
VERBOSE_LOGGING=true
DEMO_DATA_PATH=./data